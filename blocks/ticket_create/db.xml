<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.concrete5.org/doctrine-xml/0.5" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">
    <table name="btTicketCreate">
        <field name="bID" type="integer">
            <unsigned/>
            <key/>
        </field>
        <field name="thankYouMessage" type="string" size="255"/>
        <field name="submitText" type="string" size="255">
            <default value="Submit"/>
        </field>
        <field name="displayCaptcha" type="integer">
            <default value="1"/>
        </field>
        <field name="redirectCID" type="integer" size="10">
            <default value="0"/>
        </field>
    </table>
</schema>
